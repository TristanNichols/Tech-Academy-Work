# Both of these files are in the same folder
# File name: smallMathsModule.py

from random import randint

def multiplyBy5(x):
    return 5 * x

def add5(x):
    return x + 5

def randomAdd(x):
    y = randint(0, 10)
    return x + y

# File name: PythonInADay

import smallMathsModule

print smallMathsModule.multiplyBy5(3)
print smallMathsModule.add5(9)
print smallMathsModule.randomAdd(8)
